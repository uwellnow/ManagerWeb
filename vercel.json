{
  "git": {
    "deploymentEnabled": {
      "main": true,
      "dev": false,
      "feature/*": false
    }
  },
  "github": {
    "silent": true
  },
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://manage-uwellnow.com/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}