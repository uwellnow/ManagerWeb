{
  "git": {
    "deploymentEnabled": {
      "main": true,
      "dev": false,
      "feature/*": false
    }
  },
  "github": {
    "silent": true
  },
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}